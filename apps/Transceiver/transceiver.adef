sandboxed: false
start: auto
version: 1.0

executables: 
{
	transceiver = ( transceiverComponent )
}

processes:
{
	run: 
	{
		( transceiver )
	}
}

requires: {
	file: {
		/bin/sh /usr/local/bin/
		/sbin/ip /usr/local/bin/
		/sbin/ifconfig /usr/local/bin/
		/legato/systems/current/bin/kmod /usr/local/bin/
	}
}


bindings:
{
	transceiver.transceiverComponent.le_gnss -> positioningService.le_gnss
}
